<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.Controllers.RechercheLecteurController">


        <Label alignment="CENTER" layoutX="176.0" layoutY="14.0" prefHeight="42.0" prefWidth="280.0" text="Rechercher un Lecteur " textFill="#c495aa" underline="true">
            <font>
                <Font name="System Bold Italic" size="26.0" />
            </font>
        </Label>
        <Label layoutX="30.0" layoutY="72.0" prefHeight="26.0" prefWidth="118.0" text="Choix du critère">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <TableView fx:id="rechTableView" layoutX="60.0" layoutY="145.0" prefHeight="200.0" prefWidth="475.0">
            <columns>
                <TableColumn fx:id="cinRechColumn" prefWidth="120.19997787475586" text="CIN" />
                <TableColumn fx:id="nomRechColumn" prefWidth="178.0000457763672" text="Nom" />
                <TableColumn fx:id="prenomRechColumn" prefWidth="178.0" text="Prénom" />
            </columns>
        </TableView>
        <Button fx:id="rechLecteurBTN" layoutX="362.0" layoutY="105.0" mnemonicParsing="false" onAction="#rechercheLecteurSelonCritere" prefHeight="26.0" prefWidth="135.0" text="Rechercher" />
        <ChoiceBox fx:id="rechCritereChoiceBox" layoutX="148.0" layoutY="72.0" prefWidth="150.0" />
        <Label alignment="CENTER" layoutX="49.0" layoutY="108.0" prefHeight="18.0" prefWidth="80.0" text="Critère">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
        <TextField fx:id="critereChoisiTextField" layoutX="148.0" layoutY="105.0" prefHeight="26.0" prefWidth="208.0" promptText="Entrez le critère de recherche" />
        <Button fx:id="retourBTN" layoutX="252.0" layoutY="351.0" mnemonicParsing="false" onAction="#AfficherFenetreHome" prefHeight="26.0" prefWidth="92.0" text="Retour" />


</AnchorPane>
