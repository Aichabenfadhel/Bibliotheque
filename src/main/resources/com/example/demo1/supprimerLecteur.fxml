        <buttons>
            <Button fx:id="SupprimerBTN" mnemonicParsing="false" onAction="#handleSupprimerLecteurEtAbonn" text="Supprimer" />
            <Button fx:id="suppRetourBTN" layoutX="135.0" layoutY="17.0" mnemonicParsing="false" onAction="#handleAfficherFenetreLecteurs" text="Retour" />
