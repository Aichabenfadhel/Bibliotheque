<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.Controllers.LecteurController">

    <Label alignment="CENTER" layoutX="166.0" layoutY="45.0" prefHeight="44.0" prefWidth="267.0" text="Suppression d'un Lecteur " textFill="#c495aa" underline="true">
